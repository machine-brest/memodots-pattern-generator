<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>

<BorderPane stylesheets="@../styles/main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<Pane fx:id="contentPane" id="contentPane" prefHeight="500.0" prefWidth="500.0" />
	</center>
	<bottom>
		<Pane fx:id="commandPane" id="commandPane" prefHeight="60.0">
			<HBox>
				<Button fx:id="generateButton" maxWidth="Infinity" mnemonicParsing="false" text="Generate" />
			</HBox>
		</Pane>
	</bottom>
</BorderPane>
